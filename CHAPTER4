--PROJECT 4.7
--Create and document a password policy that expires the password after 60 days.

SQL> CREATE PROFILE password_expiry_profile LIMIT
  2      PASSWORD_LIFE_TIME 60
  3      PASSWORD_REUSE_TIME 365
  4      PASSWORD_GRACE_TIME 7;

